	<property name="ireport.encoding" value="UTF-8" />
	<import value="java.util.*" />
	<import value="net.sf.jasperreports.engine.*" />
	<import value="net.sf.jasperreports.engine.data.*" />

	<parameter name="pid" isForPrompting="true" class="java.lang.String"/>
	
  <queryString>select distinct v . firstviralload as cnt , v . visitdatedd as dd , v . visitdatemm as mm , v . visitdateyy as yy from vitals v , encounter e where e . patientid = $P{pid} and e . encstatus &lt; 255 and e . encountertype in (1 , 2) and e . sitecode = v . sitecode and e . patientid = v . patientid and e . visitdatedd = v . visitdatedd and e . visitdatemm = v . visitdatemm and e . visitdateyy = v . visitdateyy and e . seqnum = v . seqnum and v . firstviralload is not NULL and ltrim(v . firstviralload) != '' and v . visitdatedd is not NULL and ltrim(v . visitdatedd) != '' and v . visitdatemm is not NULL and ltrim(v . visitdatemm) != '' and v . visitdateyy is not NULL and ltrim(v . visitdateyy) != '' union all select distinct l . result as cnt , ifnull(nullif(ltrim(l . resultDateDd) , '') , l . visitdatedd) as dd , ifnull(nullif(ltrim(l . resultDateMm) , '') , l . visitdatemm) as mm , ifnull(nullif(ltrim(l . resultDateYy) , '') , l . visitdateyy) as yy from labs l , encounter e where e . patientid = $P{pid} and l . labID in (103) and e . encstatus &lt; 255 and e . encountertype = '6' and e . sitecode = l . sitecode and e . patientid = l . patientid and e . visitdatedd = l . visitdatedd and e . visitdatemm = l . visitdatemm and e . visitdateyy = l . visitdateyy and e . seqnum = l . seqnum and l . result is not NULL and ltrim(l . result) != '' and l . visitdatedd is not NULL and ltrim(l . visitdatedd) != '' and l . visitdatemm is not NULL and ltrim(l . visitdatemm) != '' and l . visitdateyy is not NULL and ltrim(l . visitdateyy) != '' order by yy desc , mm desc , dd desc; 
</queryString>

	<field name="mm" class="java.lang.String"/>
	<field name="dd" class="java.lang.String"/>
	<field name="yy" class="java.lang.String"/>
	<field name="cnt" class="java.lang.String"/>

	<variable name="viralLoadCount" class="java.lang.Integer" resetType="Report" calculation="Count">
		<variableExpression><![CDATA[$F{cnt}]]></variableExpression>
		<initialValueExpression><![CDATA[new Integer(0)]]></initialValueExpression>
	</variable>
		<background>
			<band height="0"  isSplitAllowed="true" >
			</band>
