  <step>
    <name>labs join encvalid</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>localhost</connection>
    <sql>select t.siteCode, t.patientID,
 case
  when isdate(ymdToDate(t.resultDateYy, t.resultDateMm, t.resultDateDd)) = 1
  then ymdToDate(t.resultDateYy, t.resultDateMm, t.resultDateDd)
  else e.visitDate
 end as visitDate,
t.result as cd4,
e.encounterType, e.encounter_id, e.formVersion 
from labs as t
 join encValid as e using (siteCode, patientID, visitDatedd, visitDatemm, visitDateyy, seqNum)
 join labLookup as l
where t.result is not null
 and ((t.labID = l.labID and t.labID <= 1000) or (t.labID > 1000 and lcase(t.testNameFr) = lcase(l.testNameFr) and lcase(t.sampleType) = lcase(l.sampleType)))
 and isnumeric(t.result) = 1
 and lower(l.labName) in (&apos;cd4&apos;, &apos;cd4 en mm3&apos;, &apos;cd4 en %&apos;);</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
