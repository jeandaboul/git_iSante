  <step>
    <name>encValid adults</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>localhost</connection>
    <sql>select p.patientID,
 year(e.visitDate) - p.dobYy as ageAtReg,
 e.visitDate as regDate,
 case when year(e.visitDate) - p.dobYy &lt; 14
  then date_add(p.dobYy, INTERVAL 14 year)
  else NULL end as fourteenYearsOld,
 NULL as sixWeeksOld,
 NULL as oneYearOld,
 e.encounter_id,
 e.encounterType,
 e.formVersion
from encValid e join patient p using (patientID)
where isnumeric(p.dobYy) = 1
 and isnumeric(p.dobMm) != 1
 and e.encounterType in (10, 15)</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
